grep Etot mdout.master.DPFP | head -n -4 > xx
grep Etot mdout.master.$1 | head -n -4 > yy
grep Etot mdout.mint.$1 | head -n -4 > zz
grep Etot mdout.mbond.$1 | head -n -4 > ww
xmgrace -block xx -bxy 0:3 -block yy -bxy 0:3 -block zz -bxy 0:3 -block ww -bxy 0:3 
